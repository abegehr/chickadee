{
  "name": "chickadee",
  "description": "Simple, privacy-focused web analytics you can self-host on Cloudflare.",
  "keywords": [
    "website",
    "analytics",
    "privacy",
    "self-hosted",
    "cloudflare-worker"
  ],
  "scripts": {
    "app": "pnpm --filter @chickadee/app",
    "web": "pnpm --filter @chickadee/web",
    "build": "turbo run build",
    "check": "turbo run check",
    "check-types": "turbo run check-types",
    "dev": "turbo run dev",
    "lint": "biome lint --write",
    "format": "biome format --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "turbo": "^2.5.3"
  },
  "author": "abegehr",
  "license": "MIT",
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977"
}
